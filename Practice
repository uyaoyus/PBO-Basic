public class Pbo_ {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int a = 10;
        int b = 20;
        System.out.println("Data Integer");
        System.out.println("Nilai A :" +a);
        System.out.println("Nilai B :" +b);
        double c = 19.99d;
        double d = 21.11d;
        System.out.println("Data Double");
        System.out.println("Double C :" +c);
        System.out.println("Double D :" +d);
        boolean isPlus = true;
        System.out.println("Data Boolean");
        System.out.println(isPlus);
        
        Scanner obj = new Scanner(System.in);
        System.out.println("Ganti Nilai A :");
        int a2 = obj.nextInt();
        System.out.println("Ganti Nilai B :");
        int b2 = obj.nextInt();
        System.out.println("Nilai A :" +a2);
        System.out.println("Nilai A :" +b2);
        System.out.println("Ganti Nilai Double C :");
        double c2 = obj.nextDouble();
        System.out.println("Ganti Nilai Double D :");
        double d2 = obj.nextDouble();
        System.out.println("Double C :" +c2);
        System.out.println("Double D :" +d2);
        System.out.println("isPlus : ");
        boolean isPluss = obj.nextBoolean();
    }
